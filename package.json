{
  "name": "staff-bot-js",
  "version": "1.0.0",
  "description": "A utility bot for discord staffs.",
  "main": "src/index.js",
  "scripts": {
    "test": "standard --global globalThis --global Log && node src/index.js",
    "start": "node src/index.js"
  },
  "keywords": [
    "bot"
  ],
  "author": "Jorge Luiz Souza Xavier",
  "license": "ISC",
  "dependencies": {
    "caller-id": "^0.1.0",
    "discord.js": "^12.0.0",
    "dotenv": "^8.2.0",
    "i18n-nodejs": "^3.0.0",
    "simple-node-logger": "^18.12.24"
  },
  "devDependencies": {
    "standard": "*",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1"
  },
  "standard": {
    "env": {
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "standard"
    ],
    "global": ["globalThis", "Log"],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "rules": {
      "quotes": [
        2,
        "double"
      ]
    }
  }
}
